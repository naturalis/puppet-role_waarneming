source ~/virtualenv/bin/activate

alias start="/usr/bin/sudo /usr/bin/supervisorctl start obs"
alias stop="/usr/bin/sudo /usr/bin/supervisorctl stop obs"
alias restart="/usr/bin/sudo /usr/bin/supervisorctl restart obs"

alias start-worker="/usr/bin/sudo /usr/bin/supervisorctl start obs-worker"
alias stop-worker="/usr/bin/sudo /usr/bin/supervisorctl stop obs-worker"
alias restart-worker="/usr/bin/sudo /usr/bin/supervisorctl restart obs-worker"

