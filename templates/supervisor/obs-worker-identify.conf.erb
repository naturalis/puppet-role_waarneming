[program:obs-worker-identify]
command=/home/obs/virtualenv/bin/celery -A app worker -l info -P gevent --autoscale=<%= scope['::role_waarneming::conf::supervisor_worker_identify_processes'] %> --queues=identify --hostname=obs-worker-identify@%%h --without-gossip --without-mingle --without-heartbeat
directory=/home/obs/django
environment=DJANGO_SETTINGS_MODULE='app.settings',PYTHONPATH=/home/obs/virtualenv
user=obs
autostart=true
autorestart=true
stdout_logfile=/var/log/supervisor/obs-worker-identify.log
redirect_stderr=true
stopsignal=QUIT
