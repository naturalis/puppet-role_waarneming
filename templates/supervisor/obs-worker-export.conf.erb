[program:obs-worker-export]
command=/home/obs/virtualenv/bin/celery -A app worker -l info --queues=export --concurrency=<%= scope['::role_waarneming::conf::supervisor_worker_export_processes'] %> --hostname=obs-worker-export@%%h
directory=/home/obs/django
environment=DJANGO_SETTINGS_MODULE='app.settings',PYTHONPATH=/home/obs/virtualenv
user=obs
autostart=true
autorestart=true
stdout_logfile=/var/log/supervisor/obs-worker-export.log
redirect_stderr=true
stopsignal=QUIT
