[program:obs-worker-default]
command=/home/obs/virtualenv/bin/celery -A app worker -l info --concurrency=<%= scope['::role_waarneming::conf::supervisor_worker_default_processes'] %> --hostname=obs-worker-default@%%h
directory=/home/obs/django
environment=DJANGO_SETTINGS_MODULE='app.settings',PYTHONPATH=/home/obs/virtualenv
user=obs
autostart=true
autorestart=true
stdout_logfile=/var/log/supervisor/obs-worker-default.log
redirect_stderr=true
stopsignal=QUIT
